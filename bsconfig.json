{
  "name": "gitphone",
  "version": "0.1.0",
  "reason": {
    "react-jsx": 2
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "sources": [{
    "dir": "src",
    "subdirs": true
  }],
  "bs-dependencies": [
    "reason-react",
    "bs-react-native",
    "bs-react-native-vector-icons",
    "bs-react-navigation",
    "bs-random-nav",
    "bs-fetch",
    "@glennsl/bs-json"
  ],
  "gentypeconfig": {
    "language": "flow",
    "shims": {
      "ReasonReact": "ReactShim"
    }
  },
  "refmt": 3,
  "suffix": ".bs.js",
  "namespace": true
}